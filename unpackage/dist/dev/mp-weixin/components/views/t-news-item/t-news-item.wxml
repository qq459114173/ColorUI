<view class="tui-list-view"><view data-event-opts="{{[['tap',[['bindClick',['$event']]]]]}}" class="{{['tui-list-cell',lastChild?'tui-last-child':'']}}" bindtap="__e"><view class="tui-title-box"><text class="tui-cell-title">{{entity.title}}</text></view><block wx:if="{{entity.imgArr&&entity.imgArr.length>0}}"><view class="tui-img-container"><block wx:for="{{entity.imgArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-cell-img"><image class="tui-img" src="{{item}}"></image></view></block></view></block><view class="tui-sub-title"><block wx:if="{{entity.badgeType!=0}}"><text class="{{['tui-badge',$root.m0]}}">{{entity.badgeText}}</text></block><text class="tui-sub-content">{{entity.subContent}}</text></view></view></view>